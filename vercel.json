{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "2000mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/chatbot1",
      "dest": "app.py"
    },
    {
      "src": "/",
      "dest": "app.py"
    }
  ]
}
